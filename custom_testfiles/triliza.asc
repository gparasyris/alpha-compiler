function control(c){
	//int a,b,i,j,count;
	a=0;
	b=0;
	count=0;
	for(i=0; i<3; i++){
		if(a==3){
			return 1;
		}
		if(b==3){
			return 2;
		}
		a=0;
		b=0;
		for(j=0; j<3; j++){
			if(c[i][j]=="X"){
				a++;
				count++;
			}
			if(c[i][j]=="O"){
				b++;
				count++;
			}
		}

	}
	if(a==3){
		return 1;
	}
	if(b==3){
		return 2;
	}

	if(count==9){
		return 3;
	}
	a=0;
	b=0;
	for(j=0; j<3; j++){
		if(a==3){
			return 1;
		}
		if(b==3){
			return 2;
		}
		a=0;
		b=0;		
		for(i=0; i<3; i++){
			if(c[i][j]=="X"){
				a++;
			}
			if(c[i][j]=="O"){
				b++;
			}
		}
		if(a==3){
			return 1;
		}
		if(b==3){
			return 2;
		}	
	}
	a=0;
	b=0;
	for(i=0; i<3; i++){
		if(c[i][i]=="X"){
			a++;
		}
		if(c[i][i]=="O"){
			b++;
		}
		if(a==3){
			return 1;
		}
		if(b==3){
			return 2;
		}
	}
	a=0;
	b=0;
	for(i=2; i>=0; i--){
		if(c[2-i][i]=="X"){
			a++;
		}
		if(c[2-i][i]=="O"){
			b++;
		}
		if(a==3){
			return 1;
		}
		if(b==3){
			return 2;
		}
	}
	

	return 0;
}

function print_table(t){

	for (i=0;i<3;i++){

		for(j=0;j<3;j++){
			//s=
			print(" ",t[i][j]," ");
			if(j<2)print("|");
		}
		if(i<2){
			print("\n---+---+---\n");
		}
	}
	print("\n");
}
function legal ( res,t){
	i=0;
	j=0;
	if(res<4){
		j=res-1;
		i=0;
	}
	if(res>=4 and res<7){
		j=res-4;
		i=1;
	}
	if(res>=7 and res<10){
		j=res-7;
		i=2;
	}
	return (t[i][j]);

}


